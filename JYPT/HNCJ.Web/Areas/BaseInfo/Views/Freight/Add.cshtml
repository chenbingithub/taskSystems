@model HNCJ.Model.Freight
@{
    Layout =null;
}

    <div   style="width:100%;max-width:400px;padding:30px 60px;">
		<form id="ff" method="post" action="/baseinfo/Freight/save">
            <div style="margin-bottom:20px">
                @Html.TextBoxFor(u => u.FreightName, new {@class="easyui-textbox",@style="width:100%",@data_options="label:'配送名称:',required:true" })
			</div>
			<div style="margin-bottom:20px">
                @Html.TextBoxFor(u => u.FreightPrice, new {@class="easyui-numberbox",@style="width:100%",@data_options="label:'价格:',required:true" })
			</div>
             
            
            
			@Html.HiddenFor(u=>u.ID)
			@Html.HiddenFor(u=>u.RegTime)
			@Html.HiddenFor(u=>u.ModfiedOn)
			@Html.HiddenFor(u=>u.DelFlag)
       

		</form>
	</div>
    