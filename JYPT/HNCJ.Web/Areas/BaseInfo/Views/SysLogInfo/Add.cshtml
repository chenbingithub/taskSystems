@model HNCJ.Model.SysLog
@{
    Layout =null;
}



   
    
    <div   style="width:100%;max-width:400px;padding:30px 60px;">
		<form id="ff" method="post" action="/baseinfo/sysloginfo/save">
            <div style="margin-bottom:20px">
                @Html.TextBoxFor(u => u.UserName, new {@class="easyui-textbox",@style="width:100%",@data_options="label:'用户名:',required:true" })
			</div>
			<div style="margin-bottom:20px">
                @Html.TextBoxFor(u => u.Operation, new {@class="easyui-textbox",@style="width:100%",@data_options="label:'操作:',required:true" })
			</div>
             <div style="margin-bottom:20px">
                  @Html.TextBox("OperationTime",Model.OperationTime.Value.ToString("MM/dd/yyyy hh:mm:ss"), new {@class="easyui-datetimebox",@style="width:100%",@data_options="label:'操作时间:',required:true" })
			</div>
            
            
			@Html.HiddenFor(u=>u.ID)
			@Html.HiddenFor(u=>u.UserInfoID)
			@Html.HiddenFor(u=>u.DelFlag)
       

		</form>
	</div>
    