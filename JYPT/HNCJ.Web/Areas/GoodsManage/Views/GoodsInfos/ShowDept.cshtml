@model HNCJ.Model.Dept
@{
    Layout =null;
}
<style type="text/css">
    table 
{ 
margin-left: auto; 
margin-right: auto; 
} 
</style>
<div  style="padding-top:60px;">
            <table cellpadding="5" cellspacing="10"  >
                <tr>
	    			<td>上级部门:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.ParentName, new {@class="textbox",@style="height:22px",@readonly="true" })
                        </td>
	    		
	    		</tr>
	    		<tr>
	    			<td>编码:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.deptno, new {@class="textbox",@style="height:22px",@readonly="true" })
                        </td>
	    		
	    		</tr>
                <tr>
	    			<td>名称:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.deptname, new {@class="textbox",@style="height:22px",@readonly="true" })
                        </td>
	    		
	    		</tr>
                <tr>
	    			<td>负责人:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.deptmanager, new {@class="textbox",@style="height:22px",@readonly="true" })
                        </td>
	    		
	    		</tr>
                <tr>
	    			<td>电话:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.telphone, new {@class="textbox",@style="height:22px",@readonly="true" })
                        </td>
	    		
	    		</tr>
                <tr>
	    			<td>描述:</td>
	    			<td>
                        @Html.TextBoxFor(u => u.remark, new {@class="textbox",@readonly="true",@style="width:100%;height:60px", @data_options="multiline:true" })
                        </td>
	    		
	    		</tr>
                </table>
           

	</div>