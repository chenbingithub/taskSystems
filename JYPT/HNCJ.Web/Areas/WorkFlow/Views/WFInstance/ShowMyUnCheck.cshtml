@model IEnumerable<HNCJ.Model.WFInstance>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ShowMyUnCheck</title>
    <link href="~/Content/jquery-easyui-1.5/tableStyle.css" rel="stylesheet" />
    <style type="text/css">
        .btn{ display:inline-block;text-decoration:none; margin:0; padding:0 25px; color:#fff; font-size:12px; height:20px; line-height:20px; text-align:center; }
.btn{ background:#1e7bb3; border:1px solid #02598E; border-radius:10px; vertical-align:middle; }
.btn:hover{ color:#fff; opacity:0.9;text-decoration:none; }
    </style>
</head>
<body>
    <div>
        <table width="100%" cellspacing="0">
           <tr><th>流程名</th><th>模板名</th><th>提交人</th><th>紧急程度</th><th>提交时间</th><th>状态</th><th>操作</th></tr>
           @foreach(var item in Model.OrderByDescending(u=>u.StartTime)){
           <tr><td>@item.InstanceName</td><td>@item.WFTemp.TempName</td><td>@HNCJ.Web.Models.Commcs.IDToName(item.StartBy)</td><td>@HNCJ.Common.Utils.IntToStr(item.Level)</td><td>@item.StartTime</td><td>@HNCJ.Common.Utils.ShortToStr(item.Status)</td><td><a href="/WFInstance/Details/@item.ID" class="btn" >查看</a></td></tr>
           }
               </table>
    </div>
</body>
</html>
